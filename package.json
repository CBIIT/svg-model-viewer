{
  "name": "viewer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "browserify ./public/javascripts/actions.js > ./public/javascripts/bundle.js",
    "watch": "watchify ./public/javascripts/actions.js -o ./public/javascripts/bundle.js &",
    "unwatch": "PID=$(ps | grep watchify | grep -v grep | { read i j ; echo $i ; }) ; [[ -n $PID ]] && kill $PID ; exit 0"
  },
  "dependencies": {
    "browserify": "^16.5.0",
    "cookie-parser": "~1.4.4",
    "clean-css": ">=4.1.11",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jquery": "^3.5.0",
    "leaflet": "^1.5.1",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "watchify": "^3.11.1"
  }
}
